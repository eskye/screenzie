{
    "name": "Screensharing Extension",
    "description": "Screensharing Extension for my app",
    "version": "1.0.0",
    "manifest_version": 2,
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "permissions": [
      "desktopCapture",
      "file://*/*",
      "tabs"
    ]
  }
  